passive class FromTo
  from : int
  to : int

passive class VertexPair
  a : int
  b : int

  def init(a : int, b : int) : void {
    this.a = min(a, b);
    this.b = max(a, b);
  }

  def equals(vp : VertexPair) : bool {
    (this.a == vp.a) and (this.b == vp.b)
  }

  def toInt() : int {
    this.a + this.b -- TODO better hash? function ex a +  b * 73
  }
